{
	"buildCommand": "cd path_panda_widget && npm run build",
	"outputDirectory": "path_panda_widget/dist",
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000"
				}
			]
		}
	]
}
